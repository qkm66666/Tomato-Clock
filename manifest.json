{
  "manifest_version": 3,
  "name": "Tomato",
  "version": "1.0.0",
  "description": "工作学习时间管理",
  "icons": {
    "48": "assets/64.png",
    "128": "assets/128.png"
  },
  "permissions": [
    "storage",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "window.html"
  }
}
